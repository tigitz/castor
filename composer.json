{
    "name": "jolicode/castor",
    "description": "DX oriented task runner and command launcher built with PHP",
    "keywords": ["task", "runner", "async", "parallel", "process", "castor"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "jwurtz@jolicode.com"
        },
        {
            "name": "Grégoire Pineau",
            "email": "lyrixx@lyrixx.info"
        },
        {
            "name": "Loïck Piera",
            "email": "pyrech@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Castor\\": "src/"
        },
        "files": [
            "src/functions.php",
            "src/functions-internal.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Castor\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "composer/composer": "^2.7.2",
        "jolicode/jolinotif": "^2.6.0",
        "jolicode/php-os-helper": "^0.1.0",
        "logiscape/mcp-sdk-php": "^1.0",
        "monolog/monolog": "^3.5",
        "nikic/php-parser": "^4.19.1",
        "spatie/ssh": "^1.10.1",
        "symfony/cache": "^6.4.6",
        "symfony/config": "^6.4.6",
        "symfony/console": "^6.4.6",
        "symfony/dependency-injection": "^6.4.6",
        "symfony/deprecation-contracts": "^3.4",
        "symfony/dotenv": "^6.4.4",
        "symfony/error-handler": "^6.4.6",
        "symfony/expression-language": "^6.4.3",
        "symfony/filesystem": "^6.4.6",
        "symfony/finder": "^6.4",
        "symfony/http-client": "^6.4.6",
        "symfony/monolog-bridge": "^6.4.4",
        "symfony/process": "^6.4.4",
        "symfony/string": "^6.4.4",
        "symfony/translation-contracts": "^3.4.2",
        "symfony/var-dumper": "^6.4.6",
        "symfony/yaml": "^6.4.3"
    },
    "conflict": {
        "symfony/console": "6.4.3"
    },
    "bin": [
        "bin/castor"
    ],
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.1.19"
        }
    },
    "require-dev": {
        "phpunit/php-code-coverage": "^9.2.31",
        "symfony/phpunit-bridge": "^6.4.6"
    }
}
